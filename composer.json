{
    "name": "homicity/laravel-mandrill",
    "description": "A laravel mail macro to use mandrill templates",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Homicity",
            "email": "info@homicity.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
        "mandrill/mandrill": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
        "phpunit/phpunit": "^6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Homicity\\MandrillMailable\\": "src"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Homicity\\MandrillMailable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Homicity\\MandrillMailable\\Providers\\MandrillMailableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
